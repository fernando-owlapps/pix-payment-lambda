# === Gerencianet PIX OAuth ===
CLIENT_ID=coloque_seu_client_id_aqui
CLIENT_SECRET=coloque_seu_client_secret_aqui

# === Certificados HTTPS obrigatórios para autenticação mTLS ===
CERT_PATH=certs/cert.pem
KEY_PATH=certs/key.key

# === URL do webhook PIX que receberá notificações (precisa estar pública) ===
WEBHOOK_URL=https://suaapi.com/webhook-pix

# === Cache do token OAuth ===
DYNAMODB_TABLE_NAME=pix_oauth_cache
AWS_REGION=us-east-1
